{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "types": [],
    "strict": false,
    "jsx": "react-native"
  },
  "include": [
    "src/**/*",
    "typings/**/*"
  ],
  "tstl": {
    "luaTarget": "5.3",
    "luaPlugins": [
      {
        "name": "./plugins/cet.ts"
      }
    ],
    "luaBundleEntry": "src/index.tsx",
    "luaBundle": "dist/bundle.lua"
  }
}